{
  "check-coverage": true,
  "per-file": true,
  "lines": 50,
  "statements": 50,
  "functions": 50,
  "branches": 50,
  "exclude": [
    "**/*.test-*.js",
    "logger.js"
  ],
  "reporter": [
    "lcov",
    "json",
    "text-summary"
  ],
  "temp-dir": "./coverage/.nyc_output",
  "report-dir": "./coverage"
}